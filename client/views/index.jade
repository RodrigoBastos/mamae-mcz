    section.section.section-parceiros#partners
      .container
        .row
          .col-lg-12
            h3.text-center
              | Parceiros
            hr

            #owl-demo.owl-carousel.owl-theme
              a.item(href="#", target="_blank")
                img(src="images/parceiros/amarilio.png")
              a.item(href="#", target="_blank")
